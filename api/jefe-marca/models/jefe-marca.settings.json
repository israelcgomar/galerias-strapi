  "collectionName": "jefe_marcas",
  "info": {
    "name": "JefeMarca"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "nombre": {
      "type": "string"
    },
    "telephoneNumber": {
      "type": "integer"
    },
    "extensionNumber": {
      "type": "integer"
    },
    "cellphoneNumber": {
      "type": "integer"
    },
    "email": {
      "type": "email"
    },
    "isEnabled": {
      "type": "boolean"
    },
    "avatar": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "mall": {
      "model": "mall"
    }
  }
}
